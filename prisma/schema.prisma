generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Lead {
  id        Int      @id @default(autoincrement())
  fullName  String
  phone     String
  status    String   @default("PENDING") // PENDING, CONTACTED, CLOSED
  createdAt DateTime @default(now())
}

model Content {
  id        Int      @id @default(autoincrement())
  key       String   @unique
  value     String
  updatedAt DateTime @updatedAt
}
